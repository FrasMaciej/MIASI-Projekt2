<?xml version="1.0" encoding="UTF-8"?>
<erd:Model xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:erd="http://www.example.org/erd">
  <entities name="Person">
    <attributes name="id" type="Integer"/>
    <attributes name="name" type="String"/>
    <operations name="getName">
      <parameters name="id" type="Integer"/>
      <returnType>String</returnType>
    </operations>
  </entities>
  <entities name="Address">
    <attributes name="id" type="Integer"/>
    <attributes name="street" type="String"/>
  </entities>
</erd:Model>